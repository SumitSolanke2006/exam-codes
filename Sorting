#include<iostream>
using namespace std;

void bubbleSort(int arr[], int n){
    for(int i=0; i<n-1; i++){
        for(int j=0; j<n-i-1; j++){
            if(arr[j]>arr[j+1]){
                swap(arr[j],arr[j+1]);
            }
        }
    }
}

void selectionSort(int arr[], int n){
    for(int i=0; i<n-1; i++){
        int minIndex = i;
        for(int j=i+1; j<n; j++){
            if(arr[j]<arr[minIndex]){
                minIndex = j;
            }
        }
        swap(arr[i],arr[minIndex]);
    }
}

void printArray(int arr[], int n){
    for(int i=0; i<n; i++){
        cout<<arr[i]<<" ";
    }
    cout<<endl<<endl;
}

int main(){
    int salary[100], n;
    
    cout<<"Enter number of employees:";
    cin>>n;
    
    cout<<"Enter Employee Salary:";
    for(int i=0; i<n; i++){
        cin>>salary[i];
    }
    
    cout<<"\nResult using Bubble Sort:\n"<<endl;
    bubbleSort(salary, n);
    printArray(salary, n);
    
    cout<<"\nResult using Selection Sort:\n"<<endl;
    selectionSort(salary, n);
    printArray(salary, n);
    
    if(n>=5){
        cout<<"Top 5 Salaries among salaries mentioned:";
        for(int i=n; i>n-5; i--){
            cout<<salary[i-1]<<" ";
        }
    }
    else{
        for(int i=n; i>0; i++){
            cout<<salary[i-1]<<" ";
        }
    }
}
