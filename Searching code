#include<iostream>
using namespace std;

void linearSearch(int arr[], int n, int key){
	for(int i=0; i<n; i++){
		if(arr[i]==key){
		    cout<<"The key is found at position:"<<i+1<<endl;
		    return;
		}
	}
	cout<<"Key value is not found"<<endl;
}

void bubbleSort(int arr[], int n){
	for(int i=0; i<n-1; i++){
		for(int j=0; j<n-i-1; j++){
			if(arr[j]>arr[j+1]){
				swap(arr[j],arr[j+1]);
			}
		}
	}
}


void binarySearch(int arr[], int n, int key){
	bubbleSort(arr, n);
	int low=0, high=n-1;

	while(low<=high){
		int mid=(low+high)/2;

		if(arr[mid]==key){

			cout<<"Key value find at position(in Binary):"<<mid+1<<endl;

			return;

		}

		else if(arr[mid]>key){

			high = mid-1;

		}

		else{

			low=mid+1;

		}

	}
	cout<<"Key value not found"<<endl;



}


int main(){
	int customerIDs[100], n, key;
	cout<<"Enter number of customer:"<<endl;
	cin>>n;
	
	cout<<"Enter customer ID's:";

	for(int i=0; i<n; i++){

		cin>>customerIDs[i];

	}



	cout<<"Enter customer ID to find:";

	cin>>key;



	cout<<"Linear Search_"<<endl<<endl;
	linearSearch(customerIDs, n, key);



	cout<<"_Binery Search"<<endl<<endl;
	binarySearch(customerIDs, n, key);
}
